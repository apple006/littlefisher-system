<?xml version="1.0" encoding="UTF-8" ?>
    <!DOCTYPE generatorConfiguration 
      PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
      "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <!-- 使用前，请修改classPathEntry指向的数据库连接jar包位置，修改jdbcConnection连接信息 -->
  <!-- 生成的model mapper client如果不在此工程下，targetProject指向的源码路径前需带上指向工程名 -->
  <!-- 这边引用了自定义的注释生成策略的实现类YcCommentGenerator，可以修改其中的作者名，版本号，达到你所需求的注释要求 -->
  <classPathEntry
    location="/Users/littlefisher/Documents/Repository/mysql/mysql-connector-java/5.1.38/mysql-connector-java-5.1.38.jar" />
  <context id="DB2Tables" targetRuntime="MyBatis3" defaultModelType="flat">
    <plugin type="com.yc.room1000.core.mybatis.SerializablePlugin" />
    <plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
    <commentGenerator type="com.yc.room1000.core.mybatis.YcCommentGenerator">
      <!-- 生成作者的名字 -->
      <property name="author" value="jinyanan" />
    </commentGenerator>
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://127.0.0.1:3306/engine"
      userId="root" password="root">
    </jdbcConnection>
    <javaTypeResolver type="com.yc.room1000.core.mybatis.YcJavaTypeResolver">
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>
    <javaModelGenerator targetPackage="com.yc.room1000.engine.attr.model" targetProject="src/main/java">
      <property name="enableSubPackages" value="false" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>
    <sqlMapGenerator targetPackage="com.yc.room1000.engine.attr.mapper" targetProject="src/main/java">
      <property name="enableSubPackages" value="false" />
      <property name="trimStrings" value="true" />
    </sqlMapGenerator>
    <javaClientGenerator targetPackage="com.yc.room1000.engine.attr.dao" targetProject="src/main/java"
      type="XMLMAPPER">
      <property name="enableSubPackages" value="false" />
      <property name="rootInterface" value="com.yc.room1000.core.mybatis.SuperMapper" />
    </javaClientGenerator>
    <table tableName="attr" domainObjectName="AttrDto" enableCountByExample="false" enableUpdateByExample="false"
      enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>
    <table tableName="attr_catg" domainObjectName="AttrCatgDto" enableCountByExample="false"
      enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>
    <table tableName="attr_catg_children" domainObjectName="AttrCatgChildrenDto" enableCountByExample="false"
      enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>
    <table tableName="attr_catg_state" domainObjectName="AttrCatgStateDto" enableCountByExample="false"
      enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>
    <table tableName="attr_catg_type" domainObjectName="AttrCatgTypeDto" enableCountByExample="false"
      enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>
    <table tableName="attr_state" domainObjectName="AttrStateDto" enableCountByExample="false"
      enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>
    <table tableName="attr_type" domainObjectName="AttrTypeDto" enableCountByExample="false"
      enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>
    <table tableName="attr_value" domainObjectName="AttrValueDto" enableCountByExample="false"
      enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>
    <table tableName="unit" domainObjectName="UnitDto" enableCountByExample="false" enableUpdateByExample="false"
      enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>
  </context>
</generatorConfiguration>